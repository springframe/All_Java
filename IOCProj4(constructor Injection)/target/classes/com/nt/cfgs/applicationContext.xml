<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans.xsd">
<bean id="st" class="com.nt.beans.Student">
<constructor-arg value="1001" type="int"/>
<constructor-arg value="89.98f" type="float"/>
<constructor-arg value="Raja" type="java.lang.String"/>
</bean>
<bean id="mks" class="com.nt.beans.Marks">
<constructor-arg value="100" index="1"/>
<constructor-arg value="300" index="2"/>
<constructor-arg value="100" index="3"/>

</bean>
<bean id="fclt" class="com.nt.beans.Faculty">
<constructor-arg value="100" name="id"/>
<constructor-arg value="Jani" name="name"/>
<constructor-arg value="java" name="subject"/>
</bean>

</beans>